{
    "ValidFileTypes": [
        "png",
        "jpeg",
        "jpg"
    ],
    "staticFileName": "IdCardFile.png",
    "regex": {
        "removeSpecialCharacter": "/[^0-9A-Za-z]/gi",
        "panCard": {
            "panCardNumber": "[A-Z]{5}[0-9]{4}[A-Z]",
            "dateOfBirth": "[0-9]{2}/[0-9]{2}/[0-9]{4}"
        }
    }
}